<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<ScrollPane prefHeight="600.0" prefWidth="700.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="gui.MainViewController">
  <content>
    <VBox prefHeight="599.0" prefWidth="699.0">
      <children>
        <MenuBar>
          <menus>
            <Menu mnemonicParsing="false" text="Registration">
              <items>
                <MenuItem mnemonicParsing="false" onAction="#onMenuItemSellerAction" text="Seller" fx:id="menuItemSeller" />
                <MenuItem mnemonicParsing="false" onAction="#onMenuItemDepartmentAction" text="Department" fx:id="menuItemDepartment" />
              </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Help">
              <items>
                <MenuItem mnemonicParsing="false" onAction="#onMenuItemAboutAction" text="About" fx:id="menuItemAbout" />
              </items>
            </Menu>
          </menus>
        </MenuBar>
      </children>
    </VBox>
  </content>
</ScrollPane>
